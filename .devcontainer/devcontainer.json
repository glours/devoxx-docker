{
	"name": "Go",
	"dockerComposeFile": "compose.yml",
	"service": "devcontainer",
	"workspaceFolder": "/workspaces/devoxx-docker",

	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker:2": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go",
				"usernamehw.errorlens"
			]
		},
		"settings": {
			"terminal.integrated.shell.linux": "/bin/bash"
		}
	}
}
